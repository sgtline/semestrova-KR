def main_task(file, limit):
    try:
        with open(file, 'r') as file:
            lines = file.readlines()

            for line in lines:
                bez_probiliv = line.strip()
                time, number = bez_probiliv.split(' ')
                if float(number) > float(limit):
                    print(f"Час: {time}, Вимірювання: {number}")

    except Exception as e:
        print(f"An error occurred: {e}")

if __name__ == "__main__":
    limit = input("Введіть мінімальне значення виводу: ")
    file = "information"
    main_task(file, limit)
